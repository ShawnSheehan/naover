nents/Chart/index.js","original":{"line":4,"column":21}},{"generated":{"line":145,"column":50},"source":"../src/components/Chart/index.js","original":{"line":5,"column":21}},{"name":"Chart","generated":{"line":157,"column":26},"source":"../src/components/Chart/index.js","original":{"line":4,"column":21}}],"sources":{"../src/components/Chart/index.js":"import React, { Component } from 'react';\nimport { PieChart, Pie, Legend } from 'recharts';\n\nexport default class Chart extends Component {\n  static propTypes = {};\n\n  state = {\n    cam: '',\n    sol: ''\n  };\n\n  onDaySelect = e => {\n    const { selectDay } = this.props;\n\n    this.setState({ sol: e.name });\n\n    selectDay(e.name);\n  };\n\n  onCamSelect = e => {\n    const { selectCam } = this.props;\n\n    this.setState({ cam: e.name });\n\n    selectCam(e.name);\n  };\n\n  onAllSelect = e => {\n    const { sol, cam } = this.state;\n    const { selectAll } = this.props;\n\n    this.setState({ cam: e.name });\n\n    if (sol && cam) {\n      selectAll(sol, cam);\n      this.setState({ sol: '', cam: '' });\n    } else {\n      this.onCamSelect(e);\n    }\n  };\n\n  render() {\n    const { sol, cam } = this.props;\n\n    return (\n      <PieChart width={500} height={500}>\n        <Legend verticalAlign=\"top\" height={36} width={450} />\n        <Pie\n          data={cam}\n          cx={250}\n          cy={250}\n          outerRadius={120}\n          fill=\"#8884d8\"\n          onClick={this.onAllSelect}\n          label\n          dataKey=\"value\"\n        />\n        <Pie\n          data={sol}\n          cx={250}\n          cy={250}\n          innerRadius={140}\n          outerRadius={180}\n          fill=\"#82ca9d\"\n          label\n          onClick={this.onDaySelect}\n          dataKey=\"value\"\n        />\n      </PieChart>\n    );\n  }\n}\n"},"lineCount":null}},"hash":"2fe7e8992cd989c6ae1de5b1a6d72df3","cacheData":{"env":{}}}